{
  "name": "react-starter-2018",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "fuzzy-search": "^2.0.1",
    "gsap": "^1.20.4",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-gsap-enhancer": "^0.3.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "resumablejs": "^1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "docs": "documentation lint src/**",
    "docs:build": "documentation build src/** --output docs/code/ --format html",
    "docs:serve": "documentation serve src/** --output docs/code/ --format html --watch",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "tg": "ruby tag.rb",
    "lint": "eslint . --quiet",
    "lint:all": "eslint .",
    "lint:fix": "eslint . --fix --quiet",
    "lint:sass": "sass-lint 'src/styles/**/*.scss' --verbose --no-exit",
    "s3:deploy": "aws s3 sync build/ s3://redshift-transcription-tool --delete --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/containers/**/*.js",
      "src/routes/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "documentation": "^5.3.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "jest-enzyme": "^4.0.2",
    "redux-devtools": "^3.4.1",
    "sass-lint": "^1.12.1"
  }
}
